<%= form_tag do %>
<div class="row">
	<div class="col-sm-4 col-sm-offset-4" style="text-align: center">
		<h3>Enter Your Company And CWID</h3>
	</div>
</div>
<br>
<div class="row">
	<div class="col-sm-4 col-sm-offset-4">
		<select class="form-control" name="company">
  			<option value=""></option>
  			<option value="Alpha">Alpha</option>
  			<option value="Bravo">Bravo</option>
 			<option value="Charlie">Charlie</option>
 			<option value="Delta">Delta</option>
		</select>
	</div>
</div>
<br>
<div class="row">
	<div class="col-sm-4 col-sm-offset-4">
		<input type="number" id="cwid" name="cwid" class="form-control">
	</div>
</div>
<br>
<div class="row">
	<div class="col-sm-4 col-sm-offset-4">
		<button type="submit" id="submit" name="submit" class="form-control">Enter</button>
	</div>
</div>
<% end %>
<br>
<div class="row">
	<div class="col-sm-4 col-sm-offset-4">
		<%= button_to "Admin", adminLog_path, :method => "get", class: 'form-control' %>
	</div>
</div>